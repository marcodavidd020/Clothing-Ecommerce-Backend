{
    "version": 2,
    "name": "ecommerce-api",
    "builds": [
      {
        "src": "dist/main.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": [
            "dist/**",
            "node_modules/**",
            "package.json"
          ]
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/dist/main.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "regions": ["iad1"]
  }
  